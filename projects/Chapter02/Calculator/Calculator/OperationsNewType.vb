Imports System

Public Class OperationsNewType
    ' Methods
    Public Shared Function Add(ByVal number1 As Double, ByVal number2 As Double) As Double
        Return (number1 + number2)
    End Function

    Public Shared Function Divide(ByVal number1 As Double, ByVal number2 As Double) As Double
        Return (number1 / number2)
    End Function

    Public Shared Function Multiply(ByVal number1 As Double, ByVal number2 As Double) As Double
        Return (number1 * number2)
    End Function

    Public Shared Function Subtract(ByVal number1 As Double, ByVal number2 As Double) As Double
        Return (number1 - number2)
    End Function

End Class

